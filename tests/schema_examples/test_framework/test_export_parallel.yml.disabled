# Define exports configuration
exports:
  parallel-translation:
    query: |
      SELECT * FROM documents
      LIMIT 1
    formats: ["md"]
    template: "test_template.md"
    template_config:
      template: |
        # Document: {{ filename }}
        
        ## Content
        {{ raw_content }}
        
        ## Translation
        {{ english_translation | default("Not translated yet") }}